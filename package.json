{
	"private": true,
	"scripts": {
		"clean": "rm -rf build",
		"clean:djs": "rm -rf build/djs",
		"clean:djs2": "rm -rf build/djs2",
		"clean:droff": "rm -rf build/droff",
		"build": "rm -rf build && tsc",
		"build:djs": "rm -rf build/djs && tsc -p tsconfig.djs.json",
		"build:djs2": "rm -rf build/djs2 && tsc -p tsconfig.djs2.json",
		"build:droff": "rm -rf build/droff && tsc -p tsconfig.droff.json",
		"dev": "tsc -p tsconfig.json -w --preserveWatchOutput",
		"dev:djs": "rm -rf build/djs && tsc -p tsconfig.djs.json -w --preserveWatchOutput",
		"dev:djs2": "rm -rf build/djs2 && tsc -p tsconfig.djs2.json -w --preserveWatchOutput",
		"dev:droff": "rm -rf build/droff && tsc -p tsconfig.droff.json -w --preserveWatchOutput",
		"start": "node build/djs",
		"start:djs": "node build/djs",
		"start:djs2": "node build/djs2",
		"start:droff": "node build/droff",
		"lint": "eslint --ext .ts src",
		"lint:djs": "eslint --ext .ts src/djs",
		"lint:djs2": "eslint --ext .ts src/djs2",
		"lint:droff": "eslint --ext .ts src/droff",
		"docs": "typedoc --out docs src"
	},
	"author": {
		"name": "Autumn",
		"url": "https://github.com/autumnblazey/"
	},
	"contributors": [
		{
			"name": "Christopher",
			"url": "https://github.com/ChrisFhey/"
		}
	],
	"license": "MIT",
	"engines": {
		"node": "16.x"
	},
	"dependencies": {
		"@discordjs/builders": "^0.6.0",
		"@discordjs/rest": "^0.1.0-canary.0",
		"discord-api-types": "^0.22.0",
		"discord.js": "^13.1.0",
		"dotenv": "^10.0.0",
		"droff": "^0.17.14",
		"droff-helpers": "^0.2.16",
		"droff-interactions": "^0.2.15",
		"utf-8-validate": "^5.0.5",
		"zlib-sync": "^0.1.7"
	},
	"devDependencies": {
		"@autumnblaze/eslint-config-h": "^1.0.0",
		"@types/node": "^16.7.5",
		"@types/node-fetch": "^2.5.12",
		"@types/ws": "^7.4.7",
		"@typescript-eslint/eslint-plugin": "^4.29.3",
		"@typescript-eslint/parser": "^4.29.3",
		"eslint": "^7.32.0",
		"eslint-plugin-functional": "^3.7.0",
		"ts-prune": "^0.10.0",
		"typedoc": "^0.21.8",
		"typescript": "^4.4.2"
	}
}
